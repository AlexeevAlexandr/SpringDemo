<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="collectionInjection" class="CollectionInjection.CollectionInjection">
        <property name="list">
            <list>
                <value>JSR 366</value>
                <value>JSR 371</value>
                <value>JSR 343</value>
                <value>JSR 365</value>
            </list>
        </property>

        <property name="set">
            <set>
                <value>JSR 366</value>
                <value>JSR 366</value>
                <value>JSR 343</value>
                <value>JSR 365</value>
            </set>
        </property>

        <property name="map">
            <map>
                <entry key="1" value="JSR 366"/>
                <entry key="2" value="JSR 371"/>
                <entry key="3" value="JSR 343"/>
                <entry key="4" value="JSR 365"/>
            </map>
        </property>

        <property name="properties">
            <props>
                <prop key="one"> JSR 366</prop>
                <prop key="two"> JSR 371</prop>
                <prop key="three"> JSR 343</prop>
                <prop key="four"> JSR 365</prop>
            </props>
        </property>
    </bean>
</beans>